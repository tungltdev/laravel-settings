{
    "name": "tungltdev/laravel-settings",
    "description": "Laravel 5 Persistent Settings",
    "keywords": [
        "laravel",
        "settings",
        "persistent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LE THANH TUNG",
            "email": "tunglt.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": ">=5.4"
    },
    "require-dev": {
        "illuminate/database": ">=5.1",
        "illuminate/events": ">=4.1"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Tungltdev\\LaravelSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tungltdev\\LaravelSettings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Tungltdev\\LaravelSettings\\Facades\\Settings"
            }
        }
    }
}
